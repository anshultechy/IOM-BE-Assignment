  "version": "1.0.0",
  "scripts": {
    "sequelize": "node_modules/.bin/sequelize",
    "migrate": "npx sequelize db:migrate",
    "serve": "node --inspect=0.0.0.0:3002 ./app/server.js",
    "start": "set UV_THREADPOOL_SIZE=16 && yarn migrate && node_modules/.bin/nodemon --inspect=0.0.0.0:3019 ./app/server.js",
    "dev": "set UV_THREADPOOL_SIZE=16 && yarn migrate && yarn serve",
    "pm2": "yarn migrate && pm2 set pm2-logrotate:max_size 5M && pm2-runtime ./app/server.js --raw --output ./public/logs.log",
    "pm2prod": "yarn migrate && pm2-runtime ./app/server.js",
    "prod": "set UV_THREADPOOL_SIZE=16 && yarn migrate && node ./app/server.js",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "precommit": "lint-staged",
    "test": "node_modules/.bin/mocha ./tests/index.js"
  },
